{
  "name": "@cspell/dict-html-symbol-entities",
  "version": "1.0.22",
  "description": "Html Symbol Entities dictionary for cspell.",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cspell-tools compile \"entities.txt\" -o .",
    "test": "head -n 100 \"entities.txt\" | cspell -v -c ./cspell-ext.json --local=* --languageId=html stdin",
    "prepublishOnly": "yarn test",
    "prepare": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "html-symbol-entities",
    "Html Symbol Entities",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/master/dictionaries/html-symbol-entities#readme",
  "files": [
    "entities.txt.gz",
    "cspell-ext.json"
  ],
  "gitHead": "48d10290053a5c492372ed4221cdb83c835a3bcb"
}
