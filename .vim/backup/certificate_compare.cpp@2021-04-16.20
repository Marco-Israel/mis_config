                TEST_CASE( "libcryptoservice/generators/certificate_compare" )
                {
                    SECTION( "Root Certificate compare" )
                    {

                        uint8_t argc = 0;
                        char* argv[] = {};
                        FILE * tmpFile2 = fmemopen(root_ca_1_der,
                                root_ca_1_der_len, "rb+");

                        int offset = 735;
                        uint16_t min_len = std::min(
                                root_ca_1_der_len, root_ca_2_der_len);;

                          while (offset < min_len) {
                            if ( root_ca_1_der[offset] != root_ca_2_der[offset])
                             {
                                int ret =0;
                                ret  = dumpasn1(tmpFile2,offset, argc, argv );
                                if ( -1 == ret ) {
                                    ret  = dumpasn1(tmpFile2,offset+1, argc, argv );
                                }
                                std::cout<< ret <<std::endl;
                                break;
                                offset = ret;
                             }
                                offset++;
                          }
                    }
                }
