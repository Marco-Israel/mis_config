#ifndef  DUMPASN1_H_
#define DUMPASN1_H_

/* ASN.1 item chain to hold all ASN.1 items. */
typedef struct data_offset_list{
    long offset;
    long length;
    struct data_offset_list * nextptr;
    struct data_offset_list * prevptr;
  } DATA_OFFSET_LIST;

#include <stdio.h>
extern int dumpasn1( FILE* inFile, long  offset, int argc, char *argv[] );

extern int compareasn1( FILE * fileA, FILE * fileB,
    DATA_OFFSET_LIST * offsetListA, DATA_OFFSET_LIST * offsetListB);
#endif /*DUMPASN1_H_ */



