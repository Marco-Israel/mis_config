#ifndef  DUMPASN1_H_
#define DUMPASN1_H_

/* ASN.1 item chain to hold all ASN.1 items. */
#define MAX_VALUE_SIE   (512u)

typedef struct data_offset_list{
    long offset;
    long length;
    long headerSize;
    struct data_offset_list * nextptr;
    struct data_offset_list * prevptr;
  } DATA_OFFSET_LIST;

typedef enum compare_type {
    COMPARE_CURRENT_ONLY    = (1<<0),
    COMPARE_FORWARD_ALSO    = (1<<1),
    COMPARE_BACKWARDS_ALSO  = (1<<2),
    COMPARE_FORWAD_AND_BACKWARDS = COMPARE_BACKWARDS_ALSO | COMPARE_FORWARD_ALSO
} COMPARE_TYPE;

#include <stdio.h>
extern int dumpasn1( FILE* inFile, long  offset, int argc, char *argv[] );

extern int compareasn1( FILE * fileA, FILE * fileB, FILE * outputStream,
        COMPARE_TYPE compareType);;

#endif /*DUMPASN1_H_ */



