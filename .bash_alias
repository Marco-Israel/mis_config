mmnt()
{
    while [ $# -gt 0 ]
    do
        if [ $1 == "[a-z]"? ]
        then
            sudo mount /dev/sd$1 /mnt/sd$1
        else
            sudo mount /dev/$1 /mnt/$1
        fi
        shift
    done
}

umnt()
{
    while [ $# -gt 0 ]
    do
        sudo umount /dev/$1
        shift
    done
}

vimdirdiff()
{
    # Shell-escape each path:
    DIR1=$(printf '%q' "$1"); shift
    DIR2=$(printf '%q' "$1"); shift
    vim $@ -c "DirDiff $DIR1 $DIR2"
}

